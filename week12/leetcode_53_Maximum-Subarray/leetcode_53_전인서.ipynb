   "source": [
    "nums = [-2,1,-3,4,-1,2,1,-5,4]\n",
    "# Output: 6\n",
    "# Explanation: [4,-1,2,1] has the largest sum = 6.\n",
    "\n",
    "dp = [0] * len(nums) # 이때 dp = [] 이렇게 만들어주고 이후 append 쓰면 훨씬 더 느림.\n",
    "dp[0] = nums[0]\n",
    "\n",
    "for i in range(1, len(nums)):\n",
    "    dp[i] = max(nums[i], nums[i]+dp[i-1])\n",
    "\n",
    "print(max(dp))"
   ]
  }
