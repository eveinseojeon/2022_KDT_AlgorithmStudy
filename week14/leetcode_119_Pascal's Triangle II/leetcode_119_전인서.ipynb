   "source": [
    "rowIndex = 3\n",
    "# Output: [1,3,3,1]\n",
    "\n",
    "answer = [0] * (rowIndex+1)\n",
    "\n",
    "for i in range(rowIndex+1):\n",
    "    answer[i] = [0] * (i+1)\n",
    "    answer[i][0] = 1\n",
    "    answer[i][i] = 1\n",
    "\n",
    "    for j in range(1, i):\n",
    "        answer[i][j] = answer[i-1][j-1] + answer[i-1][j]\n",
    "\n",
    "print(answer[rowIndex])"
   ]
  }
