   "source": [
    "# 부분집합을 구한다\n",
    "    def subsets(self, nums):\n",
    "        result = []\n",
    "        def dfs(start, visited):\n",
    "            result.append(visited)\n",
    "            for idx in range(start, len(nums)):\n",
    "                dfs(idx + 1, visited + [nums[idx]])\n",
    "        \n",
    "        dfs(0, [])\n",
    "        return result"
   ]
  }
